#to be true raw to digi sequence -- meanwhile prescaler placeholder
module  dtRTDPlaceholder = hltPrescaler from "HLTrigger/HLTcore/data/hltPrescaler.cfi"
replace dtRTDPlaceholder.prescaleFactor = 1

include "HLTrigger/Configuration/data/rawToDigi/DigiToRaw.cff"

sequence DTRawToDigi = { DigiToRaw, dtRTDPlaceholder }



